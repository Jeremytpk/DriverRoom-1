{
  "expo": {
    "name": "DriverRoom",
    "slug": "DriverRoom",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/logoOnly.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logoOnly.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jeremytpk.DriverRoom",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoOnly.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jeremytpk.DriverRoom",
      "googleServicesFile": "./google-services.json",
      "compileSdkVersion": 35,
      "targetSdkVersion": 35,
      "buildToolsVersion": "35.0.0"
    },
    "web": {
      "favicon": "./assets/logoOnly.png"
    },
    "extra": {
      "eas": {
        "projectId": "a6f59f54-c725-4958-9559-2ce7a997fb25"
      }
    },
    "plugins": [
      "@react-native-firebase/app", 
       [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "modularHeaders": true,
            "xcodeProps": {
              "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
            }
          }
        }
      ]
    ]
  }
}